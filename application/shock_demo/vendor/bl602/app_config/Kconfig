menu "AIIO_GPIO_CFG"
choice GPIO_SHOCK_PORT
   	bool "aiio_GPIO_SHOCK_PORT"
   	default GPIO_SHOCK_PORTB
config GPIO_SHOCK_PORTA
   	bool "AIIO_GPIO_A"
config GPIO_SHOCK_PORTB
   	bool "AIIO_GPIO_B"
endchoice
config GPIO_SHOCK_PORT
    	string
    	default AIIO_GPIO_A if GPIO_SHOCK_PORTA
    	default AIIO_GPIO_B if GPIO_SHOCK_PORTB
